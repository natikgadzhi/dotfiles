{
  "git.allowNoVerifyCommit": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 16,
  "python.autoComplete.extraPaths": [
    ".venv"
  ],
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 80,
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
  },
  "editor.renderLineHighlight": "none",
  "debug.console.fontSize": 16,
  "git.enableCommitSigning": true,
  "editor.suggestSelection": "first",
  "go.lintTool": "golint",
  "python.analysis.extraPaths": [
    ".venv"
  ],
  "editor.definitionLinkOpensInPeek": true,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on",
    "other": "on"
  },
  "gopls": {
    "formatting.gofumpt": true
  },
  "editor.fontFamily": "JetBrains Mono",
  "editor.accessibilitySupport": "off",
  "editor.fontWeight": "400",
  "editor.fontLigatures": true,
  "remote.SSH.connectTimeout": 30,
  "vsicons.dontShowNewVersionMessage": true,
  "editor.suggest.showStatusBar": true,
  "files.exclude": {
    "**/.ipynb_checkpoints": true
  },
  "explorer.confirmDragAndDrop": false,
  "editor.inlineSuggest.enabled": true,
  "go.toolsManagement.autoUpdate": true,
  "editor.formatOnSaveMode": "modifications",
  "go.enableCodeLens": {},
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "editor.fontSize": 16,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false,
    "go": true,
    "python": true
  },
  "git.autofetch": true,
  "editor.minimap.renderCharacters": false,
  "editor.guides.indentation": false,
  "editor.mouseWheelScrollSensitivity": 0.7,
  "editor.smoothScrolling": true,
  "workbench.fontAliasing": "antialiased",
  "explorer.confirmDelete": false,
  "telemetry.telemetryLevel": "off",
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.semanticHighlighting.enabled": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.fontWeightBold": "normal",
  "workbench.startupEditor": "none",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "redhat.telemetry.enabled": false,
  "remote.SSH.enableRemoteCommand": true,
  "python.terminal.focusAfterLaunch": true,
  "remote.SSH.showLoginTerminal": true,
  "remote.SSH.useLocalServer": false,
  "lldb.library": "/Applications/Xcode-beta.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.insertFinalNewline": true,
  "editor.rulers": [
    80,
    120
  ],
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
  "security.workspace.trust.untrustedFiles": "open",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.typeCheckingMode": "basic",
  "projectManager.git.baseFolders": [
    "~/src/"
  ],
  "window.dialogStyle": "custom",
  "window.commandCenter": true,
  "git.enableSmartCommit": true,
  "swift.diagnostics": true,
  "editor.formatOnSave": true,
  "lldb.launch.expressions": "native",
  "swift.searchSubfoldersForPackages": true,
  "editor.tabSize": 2,
  "editor.find.loop": false
}
